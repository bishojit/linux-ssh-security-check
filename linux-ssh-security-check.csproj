<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <RootNamespace>linux_ssh_security_check</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <!-- PublishAot requires building on target platform for cross-platform scenarios -->
    <!-- Set to false to enable cross-compilation from Windows to Linux -->
    <!-- To enable AOT, build on Linux or use condition: Condition="'$(OS)' == 'Unix'" -->
    <PublishAot>false</PublishAot>
    <InvariantGlobalization>false</InvariantGlobalization>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Docs\" />
  </ItemGroup>

</Project>
